{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@schemas(.*)$": "/src/common/schemas/$1",
    "^@dtos/(.*$)": "/src/common/dtos/$1"
  }
}
